# Location of the CXSC library
CXSC_LOC = @libcxsc_loc@

QMAKE_CXXFLAGS_WARN_ON += -Wno-unused-parameter
CONFIG += debug
QMAKE_CXXFLAGS_DEBUG += -DDEBUG
QMAKE_CXX = @cpp_call@
